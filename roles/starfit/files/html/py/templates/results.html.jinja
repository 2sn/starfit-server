{% block results %}
<br />
<div id="textWrapper">
  <b>Star:</b>
  <br />
  {{ result.star.name }}
  <br />
  Max Z: {{ config.z_max }}
  <br />
  Excluded elements: {{ exc_string }}
  <br />
  Model lower limits: {{ lol_string }}
  <br />
  <br />
  <b>Method:</b>
  <br />
  {% include 'method.html.jinja' %}
  {% block method %}
  {% endblock %}
  <br />
  <br />
  <b>Best fitting models:</b>
  <br />
  <table>
    {{ result.webtable() }}
  </table>
</div>
<br />
{% if not mail %}
  <br />
  {% for img_tag in img_tags %}
    {{ img_tag }}
  {% endfor %}
{% endif %}

{% endblock %}
